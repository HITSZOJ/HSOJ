<!DOCTYPE html>
<html lang="zh-CN" style="position: fixed; width: 100%; overflow: hidden; ">

<head>
	<meta charset="utf-8"/>
	<!-- MDUI CSS -->
	<link rel="stylesheet" href="/css/mdui.min.css"
		integrity="sha384-cLRrMq39HOZdvE0j6yBojO4+1PrHfB7a9l5qLcmRm/fiWXYY+CndJPmyu5FV/9Tw" crossorigin="anonymous" />
	<!-- MDUI JavaScript -->
	<script src="/js/mdui.min.js"
		integrity="sha384-gCMZcshYKOGRX9r6wbDrvF+TcCCswSHFucUzUPwka+Gr+uHgjlYvkABr95TCOz3A"
		crossorigin="anonymous">
	</script>
	<script src="/js/jquery-3.6.0.min.js"></script>

</head>

<body
	class='mdui-theme-primary-indigo mdui-theme-accent-pink mdui-drawer-body-left mdui-theme-layout-auto mdui-appbar-with-toolbar'>
	<header>
		<div class="mdui-appbar mdui-appbar-fixed">
			<div class="mdui-toolbar mdui-color-theme">
				<a class="mdui-btn mdui-btn-icon" mdui-drawer="{target: '#drawer'}">
					<i class="mdui-icon material-icons">menu</i>
				</a>
				<a class="mdui-typo-headline">Home</a>
				<div class="mdui-toolbar-spacer"></div>
			</div>
		</div>
	</header>
	<div id="drawer" class='mdui-drawer'>
		<div class='mdui-list'>
			<% for(var i=0; i < 10; i++) { %>
				<a href="/" class="mdui-list-item">
					<i class="mdui-list-item-icon mdui-icon material-icons">home</i>
					<div class="mdui-list-item-content">
						<%= i %>
					</div>
				</a>
			<% } %>
		</div>
	</div>